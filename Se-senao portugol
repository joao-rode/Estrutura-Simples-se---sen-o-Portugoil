programa
{
	
	funcao inicio()
	{
		cadeia nome
		inteiro idade
		
		escreva("Digite o seu nome\n")
		leia(nome)
		
		escreva ("Digite a sua idade\n") 
		leia(idade)
		
		se (idade < 18 e idade >=0){
		escreva ("Você é menor de idade!\n")
		}
		senao{
          escreva("Você é maior de idade!\n") 
		} 
          
		
	}
}
